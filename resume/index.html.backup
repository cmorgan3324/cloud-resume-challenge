<!-- Generate PDF script -->
<script>
  document.getElementById('download-btn').addEventListener('click', () => {
    const iframe = document.getElementById('pdf-frame');

    // function that calls the iframe’s generatePDF()
    const invokePDF = () => {
      if (
        iframe.contentWindow &amp;&amp;
        typeof iframe.contentWindow.generatePDF === 'function'
      ) {
        iframe.contentWindow.generatePDF();
      } else {
        console.error(
          'PDF generator function not found in iframe. Did optimized-resume.html include the script and define generatePDF()?'
        );
      }
    };

    // If the iframe is already loaded, run immediately…
    if (iframe.contentWindow.document.readyState === 'complete') {
      invokePDF();
    } else {
      // …otherwise wait for it
      iframe.onload = invokePDF;
    }
  });
</script>


